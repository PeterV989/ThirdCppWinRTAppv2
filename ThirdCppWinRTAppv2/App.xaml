<?xml version="1.0" encoding="utf-8"?>
<Application
    x:Class="ThirdCppWinRTAppv2.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:ThirdCppWinRTAppv2">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />
                <!-- Other merged dictionaries here -->
            </ResourceDictionary.MergedDictionaries>
            <!-- Other app resources here -->
            <Style x:Key="g_Button" TargetType="Button">
                <Style.Setters>
                    <Setter Property="Background" Value="#E1A300"/>
                    <Setter Property="Foreground" Value="#413D3C"/>
                </Style.Setters>
            </Style>
            <Style x:Key="f_Button" TargetType="Button">
                <Style.Setters>
                    <Setter Property="Background" Value="#58ADC9"/>
                    <Setter Property="Foreground" Value="#413D3C"/>
                </Style.Setters>
            </Style>
        </ResourceDictionary>
    </Application.Resources>
</Application>
